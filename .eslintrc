{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "fetch": false
  },
  "rules": {
    "prop-types": {"ignore": ["dispatch"]}
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}