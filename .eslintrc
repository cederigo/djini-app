{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "fetch": false
  },
  "rules": {
    "react/prop-types": [2, {"ignore": ["dispatch"]}],
    "comma-dangle": [2, "always-multiline"]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}