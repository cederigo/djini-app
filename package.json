  "dependencies": {
    "google-libphonenumber": "^1.0.20",
    "immutable": "^3.7.6",
    "lodash": "^4.8.2",
    "parse": "^1.8.5",
    "react": "^15.0.2",
    "react-native": "^0.26.0",
    "react-native-contacts": "^0.2.5",
    "react-native-router-flux": "3.26.1",
    "react-native-share": "^1.0.9",
    "react-native-swipeout": "atvenu/react-native-swipeout#patch-1",
    "react-native-vector-icons": "^2.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-jest": "^12.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-react-native": "^1.8.0",
