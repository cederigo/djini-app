{
  "name": "Wishmaster",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --no-cache",
    "test:watch": "npm run test -- --watch"
  },
  "jest": {
    "setupEnvScriptFile": "node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "immutable"
    ]
  },
  "dependencies": {
    "google-libphonenumber": "^1.0.17",
    "immutable": "^3.7.6",
    "lodash": "^4.8.2",
    "parse": "^1.8.3",
    "react-native": "^0.24.0",
    "react-native-contacts": "^0.2.4",
    "react-native-router-flux": "^3.24.0",
    "react-native-share": "^1.0.9",
    "react-native-swipeout": "^2.0.12",
    "react-native-vector-icons": "^1.3.4",
    "react-redux": "^4.4.5",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-jest": "^11.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-react-native": "^1.6.0",
    "jest-cli": "^11.0.2",
    "react": "^0.14.8"
  }
}
